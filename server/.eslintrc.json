{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "id-length": [2, {"min": 2, "max": 30, "exceptions": ["a", "n", "i","j"]}],
      "quotes": "off",
      "require-jsdoc": "off"
    }
}
